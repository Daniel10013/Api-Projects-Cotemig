{
    "compilerOptions": {
        "module": "ESNext",
        "esModuleInterop": true,
        "target": "ESNext",
        "moduleResolution": "Node",
        "outDir": "dist",
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "isolatedModules": false,
        "strict": true,
        "noImplicitThis": false,
        "noImplicitAny": true,
        "inlineSourceMap": true
    },
    "ts-node": {
        "esm": true
    },
    "lib": [
        "esnext"
    ]
}